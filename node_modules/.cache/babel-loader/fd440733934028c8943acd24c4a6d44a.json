{"ast":null,"code":"import axios from 'axios';\nimport MovieCard from './components/MovieCard.vue';\nimport Search from './components/Search.vue';\nimport { ref, onMounted } from 'vue';\nexport default {\n  name: 'App',\n\n  data() {\n    return {\n      title: [],\n      type: [],\n      year: [],\n      poster: [],\n      search: '',\n      mtitle: ''\n    };\n  },\n\n  components: {\n    MovieCard,\n    Search\n  },\n  methods: {\n    selectMovie() {\n      const apiKey = 'fac1af98';\n      const id = 'tt3896198';\n      let mtitle = '&s=' + this.mtitle;\n      let titlebox = this.title;\n      let imgbox = this.poster;\n      let typebox = this.type;\n      let yearbox = this.year;\n      this.mtitle = this.search;\n      axios.get(`http://www.omdbapi.com/?i=${id}&apikey=${apiKey}${mtitle}`).then(결과 => {\n        for (let i = 0; i < 결과.data.Search.length; i++) {\n          titlebox.push(결과.data.Search[i].Title); // 영화 제목\n\n          typebox.push(결과.data.Search[i].Type); // 영화 출시일\n\n          yearbox.push(결과.data.Search[i].Year); // 영화 장르\n\n          imgbox.push(결과.data.Search[i].Poster); // 영화 포스터\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,SAASC,GAAT,EAAeC,SAAf,QAAgC,KAAhC;AACA,eAAe;EAEbC,IAAI,EAAE,KAFO;;EAGbC,IAAI,GAAE;IACN,OAAO;MACFC,KAAI,EAAI,EADN;MAEFC,IAAG,EAAI,EAFL;MAGFC,IAAG,EAAI,EAHL;MAIFC,MAAK,EAAI,EAJP;MAKFC,MAAK,EAAI,EALP;MAMFC,MAAK,EAAI;IANP,CAAP;EASC,CAbY;;EAgBbC,UAAU,EAAE;IACVZ,SADU;IAEVC;EAFU,CAhBC;EAuBbY,OAAO,EAAE;IAEJC,WAAW,GAAE;MAChB,MAAMC,MAAK,GAAI,UAAf;MACA,MAAMC,EAAC,GAAI,WAAX;MACA,IAAIL,MAAK,GAAI,QAAQ,KAAKA,MAA1B;MACF,IAAIM,QAAO,GAAI,KAAKX,KAApB;MACF,IAAIY,MAAK,GAAI,KAAKT,MAAlB;MACA,IAAIU,OAAM,GAAI,KAAKZ,IAAnB;MACA,IAAIa,OAAM,GAAI,KAAKZ,IAAnB;MAEA,KAAKG,MAAL,GAAc,KAAKD,MAAnB;MAGCX,KAAK,CAACsB,GAAN,CAAW,6BAA4BL,EAAG,WAAUD,MAAO,GAAEJ,MAAO,EAApE,EAAuEW,IAAvE,CAA6EC,EAAC,IAAK;QACjF,KAAI,IAAIC,IAAI,CAAZ,EAAeA,IAAID,EAAE,CAA